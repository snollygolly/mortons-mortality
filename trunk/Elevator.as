/*The Elevator class handles the elevator.  It does the following:*Handles arrow and space key input*Checks for door collisions (with Elevator)*Draws doors*Moves Elevator and checks for map collisions*/package{	import flash.display.*	import flash.events.*	import flash.ui.*	import flash.filters.*	import flash.utils.Timer;    import flash.events.TimerEvent;    import flash.text.TextField;	public class Elevator extends MovieClip	{		//morton has this static property so he can be referenced from any class easily 		static public var main;		 		public function Elevator(x_, y_)		{				main = this;			x = x_;			y = y_;						elevatorHit.mouseEnabled=false;			elevatorHit.alpha=0;			hitArea=elevatorHit; //for collisions					}					}}